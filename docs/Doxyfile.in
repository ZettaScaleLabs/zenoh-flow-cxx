OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@/doc_doxygen/
INPUT                  = @CMAKE_SOURCE_DIR@/src/ @CMAKE_SOURCE_DIR@/include/ @CMAKE_CURRENT_SOURCE_DIR@/../docs

PROJECT_NAME           = "Zenoh Flow Cxx wrappers"
